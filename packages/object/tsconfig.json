{
  "compilerOptions": {
    "moduleResolution": "node",
    "module": "commonjs",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowJs": false,
    "checkJs": false,
    "noEmitOnError": false,
    "target": "esnext",
    "sourceMap": true,
    "noImplicitAny": false,
    "declaration": true,
    "strict": true,
    "strictBindCallApply": true,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": false,
    "strictNullChecks": false,
    "outDir": "./dist",
    "baseUrl": "."
  },
  "disableWarnings": false,
  "include": [
    "src"
  ],
  "exclude": [
    "**/node_modules/*",
    "**/test/*",
    "dist"
  ],
  "references": [
  ]
}
